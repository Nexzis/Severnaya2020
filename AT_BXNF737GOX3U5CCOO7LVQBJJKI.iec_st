                    <Variable UUID="E04D68884A1778DABEEDAD9A34B8E03E" Name="CAT" Type="DINT" TypeUUID="AF9665534C6D5BDEF9AE77B026938B65" Usage="input" />
                    <Variable UUID="2AB459674DDE9133A38993AE1D8E58F8" Name="STATUS" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
                    <Variable UUID="0D9395C04070A398B69B9F9EF106E81A" Name="TEXT" Type="STRING" TypeUUID="38FDDE3B442D86554C56C884065F87B7" Usage="input" />
                    <Variable UUID="E67999824CE0FEFEED70E08A5BEDAE57" Name="TEXT2" Type="STRING" TypeUUID="38FDDE3B442D86554C56C884065F87B7" Usage="input" />
                    <Variable UUID="DE0E679A4297816FE367CF98E74DC572" Name="i" Type="INT" TypeUUID="47B58C4E4726C8B230965EBE77F1169E" Usage="internal" />
                    <Variable UUID="8724C26D475B1BF1B546E4B792C98FD3" Name="TEXT1" Type="STRING" TypeUUID="38FDDE3B442D86554C56C884065F87B7" Usage="internal" />
                    <Variable UUID="0402A2CC4CBEBB9AFC19A4983E442FD0" Name="STATUS_OLD" Type="BOOL" TypeUUID="EC797BDD4541F500AD80A78F1F991834" Usage="input" />
